        "@angular/platform-browser": "~4.0.0",
        "@angular/platform-browser-dynamic": "~4.0.0",
        "@angular/router": "~4.0.0",
        "angular-in-memory-web-api": "~0.3.0",
        "boom": "^7.2.0",
        "bootstrap": "^4.1.3",
        "core-js": "^2.4.1",
        "cryptiles": "^4.1.2",
        "debug": "^3.1.0",
        "fresh": "^0.5.2",
        "hoek": "^5.0.3",
        "jquery": "^3.3.1",
        "lodash": "^4.17.11",
        "mime": "^2.3.1",
